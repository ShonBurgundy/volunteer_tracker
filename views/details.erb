<h2>Project: <%= @project.title %></h2>

<% if @volunteers.any? %>

  <ul>
    <% @volunteers.each do |volunteer| %>
      <li><a href="/volunteers/<%= volunteer.id %>"><%= volunteer.name %></a></li>
    <% end %>
  </ul>
<% else %>
  <p> There are no volunteers for this project yet! </p>
<% end %>

<a href="/projects/<%= @project.id %>/edit">Edit Project</a>
<br><a href="/">Return to homepage.</a>